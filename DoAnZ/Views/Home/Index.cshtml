    <div class="container-fluid fruite py-5">
        <div class="container py-5">
            <div class="tab-class text-center">
                <div class="row g-4">
                    <div class="col-lg-4 text-start">
                        <h1>ALL PRODUCTS</h1>
                    </div>



                    <div class="tab-content">
                        <div id="tab-1" class="tab-pane fade show p-0 active">
                            <div class="row g-4" id="productContainer">
                                @foreach (var product in Model)
                                {
                                    <div class="col-md-6 col-lg-4 col-xl-3">
                                        <div class="rounded position-relative fruite-item">
                                            <div class="fruite-img">
                                                <a href="@Url.Action("Details", "Home", new { id = product.Id })">
                                                    <!-- Thêm đường dẫn và truyền id sản phẩm -->
                                                    <img src="@Url.Content("~/images/" + product.ImageUrl)" class="img-fluid w-100 rounded-top img-style" alt="Product Image">
                                                </a>
                                            </div>
                                            <div class="text-white bg-secondary px-3 py-1 rounded position-absolute" style="top: 10px; left: 10px;">
                                                @{
                                                    var categoryName = string.Empty;
                                                    if (product.Category != null)
                                                    {
                                                        categoryName = product.Category.Name;
                                                    }
                                                }
                                                @categoryName
                                            </div>
                                            <div class="p-4 border border-secondary border-top-0 rounded-bottom">
                                                <h4>@product.Name</h4>

                                                <div class="d-flex justify-content-between flex-lg-wrap">
                                                    @*  <span class="product-price text-muted">Price: </span>
                                                <p class="text-primary mb-0">@product.Price.ToString("#,##0") Vnđ</p>
                                                <a asp-controller="ShoppingCart" asp-action="AddToCart" asp-route-productId="@product.Id" asp-route-quantity="@numOfQuantity">
                                                <button class="btn btn-outline-dark">
                                                <i class="bi-cart-fill me-1"></i> Add to Cart
                                                </button>
                                                </a> *@
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }

                            </div>
                            <div class="text-center mt-4">
                                <button id="loadMoreBtn" class="btn btn-primary">Xem thêm</button>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>

