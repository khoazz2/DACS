        <script>
            $(document).ready(function () {
                const products = $(".fruite-item");
                const loadMoreBtn = $("#loadMoreBtn");

                let visibleCount = 8; // Initial number of visible products
                const increment = 4; // Number of products to load each time

                // Function to toggle visibility of products
                function toggleProducts() {
                    let count = 0;
                    products.each(function (index, product) {
                        if (count < visibleCount) {
                            $(product).removeClass("hidden");
                        } else {
                            $(product).addClass("hidden");
                        }
                        count++;
                    });
                }

                // Initial toggle
                toggleProducts();

                // Function to load more products
                function loadMore() {
                    visibleCount += increment;
                    toggleProducts();
                    // Hide load more button if all products are visible
                    if (visibleCount >= products.length) {
                        loadMoreBtn.hide();
                    }
                }

                // Event listener for load more button
                loadMoreBtn.click(loadMore);
            });
        </script>
        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

